     42          // See bl_user_entry.h for documentation of this function.

   \                                 In section .text, align 2, keep-with-next
     43          void bootloader_user_entry(void *arg)
     44          {
     45          #if BL_TARGET_ROM || BL_TARGET_FPGA
     46          
     47              // For ROM targets, we set the RCM_FM bits to force the ROM to run out of
     48              // reset, then perform a reset. This provides a guaranteed clean system.
     49              RCM_BWR_FM_FORCEROM(RCM, 0x3);
     50              NVIC_SystemReset();
     51          
     52          #elif BL_TARGET_FLASH
     53          
     54              // For flash-resident configurations, just call the reset handler as the entry point.
     55              Reset_Handler();
     56          
     57          #elif !BL_TARGET_RAM
     58          #error "Undefined target type"
     59          #endif // BL_TARGET_ROM || BL_TARGET_FPGA
     60          }
   \                     bootloader_user_entry: (+1)
   \   00000000   0x4770             BX       LR               ;; return
