     61          static void init_hardware(void)
     62          {
     63              SIM->SCGC5 |= (SIM_SCGC5_PORTA_MASK | SIM_SCGC5_PORTB_MASK | SIM_SCGC5_PORTC_MASK | SIM_SCGC5_PORTD_MASK |
     64                             SIM_SCGC5_PORTE_MASK);
     65          
     66              SIM->SOPT2 |= SIM_SOPT2_PLLFLLSEL_MASK; // set PLLFLLSEL to select the PLL for this clock source
     67          
     68              uint8_t i;
     69              for (i = 0; i < LED_COUNT; i++)
     70              {
     71                  // Enable the LED pins GPIO
     72                  led_port[i]->PCR[led_offset[i]] = PORT_PCR_MUX(1);
     73                  // Set ports to outputs
     74                  led_gpio[i]->PDDR |= (1 << led_offset[i]);
     75              }
     76          }
     77          
     78          static void led_toggle(uint32_t leds)
     79          {
     80              uint8_t i = 0;
     81              // led OFF
     82              for (i = 0; i < LED_COUNT; i++)
     83              {
     84                  led_gpio[i]->PDOR |= (uint32_t)(1 << led_offset[i]);
     85              }
     86              // led ON
     87              led_gpio[leds]->PDOR &= (uint32_t) ~(1 << led_offset[leds]);
     88          }
